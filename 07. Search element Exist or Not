#include<stdio.h>
#include<conio.h>
#define Max_size 100

int main()
{
	int arr[Max_size 100]
	int size, i, search, found;
	printf(" Enter size of Array : ");
	scanf("%d", &size);
	printf(" Enter elements in Array :");
	
	for(i=0; i<size; i++)
	{
		scanf("%d", &search);
			found = 0;
		for(i=0; i<size; i++)
		{
			if(arr[i]==search)
			{
				found = 1;
				break;
			}
		}
	}
	if(found==1)
	{
		printf(" \n%d is found at position %d ", search, i+1);
	}
	else
	{
		printf(" \n%d is not found in the array ", search);
	}
	return 0;
}
